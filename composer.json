{
    "name": "squatto/grafite-crudmaker",
    "description": "An incredibly powerful and some say magical CRUD maker for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Scott Carpenter",
            "email": "scott@payforstay.com",
            "role": "Developer"
        },
        {
            "name": "Matt Lantz",
            "email": "mattlantz@gmail.com",
            "role": "Original package developer"
        }
    ],
    "require": {
        "php": "^7.3",
        "doctrine/dbal": "^2.9",
        "illuminate/support": "^7.0",
        "squatto/grafite-formmaker": "^1.5"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "mikey179/vfsstream": "^1.6",
        "orchestra/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Grafite\\CrudMaker\\": "src/"
        },
        "files": [
            "src/Helpers/namespace.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Grafite\\CrudMaker\\CrudMakerProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
